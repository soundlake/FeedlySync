{
	"manifest_version":2,
	"name":"FeedlyTB",
	"description":"Feedly on Thundebird",
	"version":"0.1",
	"author":"Sinyeol An",
	"browser_specific_settings": {
		"gecko":{
			"id":"FeedlyTB@soundlake",
			"strict_min_version":"78.0"
		}
	},
	"browser_action": {
		"allowed_spaces": [],
		"default_popup": "popup.html",
		"default_title": "FeedlyTB"
	},
	"permissions": [
		"menus",
		"tabs"
	],
	"icons": {
        "64": "images/icon64.png",
        "32": "images/icon32.png",
        "16": "images/icon16.png"
    }
}