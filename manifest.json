{
	"manifest_version":2,
	"name":"FeedlyTB",
	"description":"Feedly on Thundebird",
	"version":"0.1",
	"author":"Sinyeol An",
	"browser_specific_settings": {
		"gecko":{
			"id":"FeedlyTB@soundlake",
			"strict_min_version":"78.0"
		}
	},
	"browser_action": {
		"allowed_spaces": [],
		"default_title": "FeedlyTB"
	},
	"permissions": [
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
        "64": "images/icon64.png",
        "32": "images/icon24BW.png",
        "16": "images/icon.png"
    }
}